{
  "name": "ai-customer-support-chatbot",
  "version": "1.0.0",
  "description": "AI-powered customer support chatbot with responsive frontend",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:api\"",
    "dev:frontend": "vite --port 3000",
    "dev:api": "node dev-api-server.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run build && node test-api.js && node test-kb-implementation.js",
    "test:integration": "npm run build && node test-react-integration.js",
    "deploy": "netlify deploy --prod",
    "deploy:preview": "netlify deploy",
    "logs": "netlify logs",
    "env:pull": "netlify env:pull .env.local",
    "env:add": "netlify env:set",
    "setup": "node scripts/setup.js",
    "validate": "node scripts/validate-deployment.js",
    "validate:react": "node validate-react-deployment.cjs",
    "health-check": "node scripts/health-check.js",
    "backup-kb": "node scripts/backup-knowledge-base.js",
    "restore-kb": "node scripts/restore-knowledge-base.js"
  },
  "keywords": [
    "ai",
    "chatbot",
    "customer-support",
    "responsive",
    "netlify"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^18.3.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "recharts": "^3.2.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.4.20"
  }
}